        {{#if msg}}
            <div class="alert alert-danger alert-dismissible">
            <a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a>
            <strong>{{msg}}</strong>
        </div>
        {{/if}}
    
        <table class="table table-hover">
            <tr>
                <th>eid</th>
                <th>ename</th>
                <th>salary</th>
                <th>address</th>
                <th>Profile-pic</th>
            </tr>
            {{#each data}}
            <tr>
                <td>{{this.eid}}</td>
                <td>{{this.ename}}</td>
                <td>{{this.salary}}</td>
                <td>{{this.address}}</td>
                <td><img src='upload/{{this.profilepic}}' height="50" width="50"/></td>
                <td><a href="/DeleteEmp?empid={{this.eid}}"  onClick="return confirm('Deleted????')"  
                class="glyphicon glyphicon-trash"></a></td>
                <td><a href="/ShowEmp?empid={{this.eid}}"  class="glyphicon glyphicon-edit"></a></td>
            </tr>
            {{/each}}
        </table>